{
  "heredoc": {
    "prefix": "heredoc",
    "body": [
      "cat << EOF > file.txt",
      "\t$0",
      "EOF"
    ],
    "description": "Inserts a heredoc redirecting into a file"
  },
  "shellcheck_disable": {
    "prefix": "shellcheck_disable",
    "body": [
      "# shellcheck disable=SC$0"
    ],
    "description": "Inserts a shellcheck disable directive"
  },
  "for-in-array": {
    "prefix": "for-in-array",
    "body": [
      "for $1 in \"\\${$0[@]\\}\"; do",
      "done"
    ],
    "description": "Inserts for loop over bash array"
  }
}
